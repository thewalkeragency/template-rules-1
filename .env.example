# Environment variables for the project
# Copy this file to .env and fill in your actual values.

# --- API Keys ---
OPENAI_API_KEY="your_openai_api_key_here"
ANTHROPIC_API_KEY="your_anthropic_api_key_here"
# Add other service API keys as needed

# --- Database ---
# Example for PostgreSQL: DATABASE_URL="postgresql://user:password@host:port/dbname"
DATABASE_URL=""

# --- Application Settings ---
# A secret key for signing sessions and other security purposes
SECRET_KEY="a_strong_and_random_secret_key"
LOG_LEVEL="INFO"

# --- Deployment & Infrastructure ---
DIGITALOCEAN_ACCESS_TOKEN="your_digitalocean_access_token_here"
NGROK_AUTH_TOKEN="your_ngrok_auth_token_here"
